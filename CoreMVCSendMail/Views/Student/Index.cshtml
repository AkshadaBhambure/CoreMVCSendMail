@model IEnumerable<Tblstudent>
@{
    ViewData["Title"] = "Index";
}

<h1>Student List</h1>
<table class="table">
    <thead>
        <tr>
            <th>Student Id</th>
            <th>Student Name</th>
            <th>Qualification</th>
            <th>Percentage</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Rno</td>
                <td>@item.Name</td>
                <td>@item.Qualification</td>
                <td>@item.Percentage</td>
            </tr>
        }
    </tbody>
</table>
